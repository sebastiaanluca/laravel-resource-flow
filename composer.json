{
    "name": "sebastiaanluca/laravel-resource-flow",
    "type": "library",
    "description": "Quickly scaffold access to a resource.",
    "keywords": [
        "laravel",
        "resource",
        "module",
        "eloquent",
        "model",
        "migration",
        "controller",
        "validator",
        "route",
        "router"
    ],
    "homepage": "https://github.com/sebastiaanluca/laravel-resource-flow",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Luca",
            "email": "hello@sebastiaanluca.com",
            "homepage": "https://www.sebastiaanluca.com",
            "role": "Author"
        }
    ],
    "require": {
        "php": ">=7.1",
        "laravel/framework": "^5.5",
        "sebastiaanluca/laravel-helpers": "^1.0"
    },
    "require-dev": {
        "kint-php/kint": "^2.1",
        "orchestra/testbench": "^3.4.2",
        "phpunit/phpunit": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "SebastiaanLuca\\Flow\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SebastiaanLuca\\Flow\\Tests\\": "tests"
        }
    },
    "suggest": {
        "nwidart/laravel-modules": "Organize your domain logic into logical modules."
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SebastiaanLuca\\Flow\\Providers\\ResourceFlowServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
